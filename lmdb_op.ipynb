    "db_path = \"ddata/MTH_DB/MTH1200\"\n",
    "env = lmdb.open(db_path)\n",
    "with env.begin(write=False) as txn:\n",
    "    length = txn.stat()['entries']\n",
    "    print(length)\n",
    "    keys = list(txn.cursor().iternext(values=False))\n",
    "    print(len(keys))\n",
    "env.close()"
